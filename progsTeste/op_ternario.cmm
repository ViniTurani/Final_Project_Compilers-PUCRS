int a;
int b;
int c;
int d;
int r1;
int r2;
int r3;
int r4;

void main() {

    a = 1;
    b = 2;
    r1 = (a ? 1 : b);
    write("r1 = ", r1);

    a = 0;
    b = 2;
    r2 = (a ? 1 : b);
    write("r2 = ", r2);

    c = 10;
    d = 20;
    a = 0;
    r3 = (a ? ++d : ++c);  
    write("c = ", c);      
    write("d = ", d);   
    write("r3 = ", r3);    

    a = 1;
    b = 0;
    r4 = (a ? (b ? 100 : 200) : 300);
    write("r4 = ", r4);
}